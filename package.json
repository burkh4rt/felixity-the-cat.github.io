{
  "author": "Michael C. Burkhart",
  "browserslist": [
    "defaults"
  ],
  "description": "Michael's personal website",
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "clean-css": "^4.2.3",
    "clean-css-cli": "^4.3.0",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "imagemin-cli": "^6.0.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "npm": "^6.14.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "serve": "^11.3.2",
    "svgo": "1.3.2",
    "imagemin-webp": "^6.0.0"
  },
  "engines": {
    "node": "^12.13.1"
  },
  "license": "UNLICENSED",
  "name": "@felixity-the-cat/felixity-the-cat.github.io",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:felixity-the-cat/felixity-the-cat.github.io.git"
  },
  "scripts": {
    "autoprefixer": "postcss -u autoprefixer -r dist/*.css",
    "scss": "node-sass --output-style compressed --include-path ./node_modules/ --include-path ./src src/main.scss -o dist/",
    "cleancss": "cleancss -O1 'all:on;specialComments:0' -O2 'all:on' -o dist/main.css dist/main.css",
    "build:css": "run-s scss autoprefixer cleancss",
    "build:png": "imagemin 'assets/png/*' --out-dir=dist/png/",
    "build:svg": "svgo -q --multipass assets/svg/*.svg -o dist/svg/",
    "build:html": "html-minifier --collapse-whitespace --remove-comments --remove-tag-whitespace --use-short-doctype --minify-css true --minify-js true --input-dir src/ --output-dir ./ --file-ext html",
    "build:webp": "imagemin --plugin=webp --plugin.webp.quality=100 --plugin.webp.method=6 'assets/png/*' --out-dir=dist/png/",
    "pretty:html": "prettier --print-width 110 --write src/*.html",
    "pretty:scss": "prettier --print-width 110 --write 'src/**/*.scss'",
    "build": "run-p build:**",
    "pretty": "run-p pretty:*",
    "make": "run-s build pretty"
  },
  "version": "1.0.2"
}
